@model IEnumerable<IEProject_Final.Models.School>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Suburb)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PrimaryNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SecondaryNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SpecialNo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Suburb)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PrimaryNo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SecondaryNo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SpecialNo)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
